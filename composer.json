{
    "name": "robuust/craft-commerce-coinbase",
    "description": "Coinbase integration for Craft Commerce",
    "version": "2.0.0",
    "type": "craft-plugin",
    "keywords": [
        "coinbase",
        "commerce",
        "craftcms",
        "omnipay",
        "yii2"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robuust",
            "homepage": "https://robuust.digital/"
        }
    ],
    "support": {
        "email": "support@robuust.digital",
        "issues": "https://github.com/robuust/craft-commerce-coinbase/issues?state=open",
        "source": "https://github.com/robuust/craft-commerce-coinbase",
        "docs": "https://github.com/robuust/craft-commerce-coinbase",
        "rss": "https://github.com/robuust/craft-commerce-coinbase/commits/master.atom"
    },
    "require": {
        "craftcms/cms": "^4.0",
        "craftcms/commerce": "^4.0",
        "craftcms/commerce-omnipay": "^4.0",
        "zek/omnipay-coinbase": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "robuust\\coinbase\\": "src/"
        }
    },
    "extra": {
        "name": "Coinbase for Craft Commerce",
        "handle": "commerce-coinbase"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
